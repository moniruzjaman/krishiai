# Backend Production Environment Variables Template
# Copy this to Vercel Environment Variables dashboard

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
HF_TOKEN=your_huggingface_token_here
OPENAI_API_KEY=your_openai_api_key_here

# Application Settings
APP_NAME=Krishi AI Backend
APP_VERSION=1.0.0
DEBUG=False
ENVIRONMENT=production
SECRET_KEY=generate_a_new_secure_secret_key_for_production

# CORS - Update with your deployed frontend URL
ALLOWED_ORIGINS=https://your-frontend.vercel.app,https://www.your-domain.com

# Server Configuration
HOST=0.0.0.0
PORT=8000
